A,KAAK,EAAC,qBAYI;AAXT,IAAA,IAAI,EAAE,KAAA,CAAA,KAWG;AAVT,IAAA,KAAK,EAAE,QAAA,CAAA,KAUE;AATT,IAAA,KAAK,EAAA;AAAA,MAAA,IAAA,EAAA,QAAA;AAAA,MAAA,IAAA,EAA0B,QAAA,CAAA;AAA1B,KASI;AART,IAAA,OAAK,EAAE,QAAA,CAAA,KAQE;AAPV,gBAAS;AAOC,GAbZ,E;AAQa,IAAA,IAAI,EAAA,QAAA,CACb;AAAA,UADiB,OACjB,QADiB,OACjB;AAAA,aADwB,CACxB,YAAA,CAEM,KAFN,EAAA,UAAA,EAEM,gBAAA,CADD,OAAO,CAAC,IACP,CAFN,EACiB;AAAA;AADjB,OADwB,CACxB;AAAA,KADa,C;;;;GARjB,E;;AAAA,I,qCAAA,CAcI,CAhBN,CAqCI,EAnBJ,YAAA,CAcM,KAdN,EAAA,UAAA,EAcM,CAbJ,UAaI,EAZJ,YAAA,CAWY,oBAXZ,EAWY;AAVV,IAAA,KAAK,EAAC,qBAUI;AATT,IAAA,IAAI,EAAE,KAAA,CAAA,KASG;AARV,IAAA,KAAK,EAAC,QAQI;AAPV,gBAAS;AAOC,GAXZ,E;AAMa,IAAA,IAAI,EAAA,QAAA,CACb;AAAA,UADiB,OACjB,SADiB,OACjB;AAAA,aADwB,CACxB,YAAA,CAEM,KAFN,EAAA,UAAA,EAEM,gBAAA,CADD,OAAO,CAAC,IACP,CAFN,EACiB;AAAA;AADjB,OADwB,CACxB;AAAA,KADa,C;;;;GANjB,E;;AAAA,I,QAAA,CAYI,CAdN,CAmBI,EAHJ,YAAA,CAA4D,uBAA5D,EAA4D;AAA9C,IAAA,KAAK,EAAC,OAAwC;AAA/B,IAAA,KAAK,EAAE,KAAA,CAAA,KAAwB;AAAjB,IAAA,KAAK,EAAC;AAAW,GAA5D,E,IAAA,E;;AAAA,I,SAAA,CAGI,EADJ,YAAA,CAA4D,uBAA5D,EAA4D;AAA9C,IAAA,KAAK,EAAC,OAAwC;AAA/B,IAAA,KAAK,EAAE,KAAA,CAAA,KAAwB;AAAjB,IAAA,KAAK,EAAC;AAAW,GAA5D,E,IAAA,E;;AAAA,I,SAAA,CACI,CAtCN,C","sourcesContent":["<template>\n  <div class=\"row\">\n    <div class=\"col-3\">\n      <h3>Draggable 1</h3>\n      <draggable\n        class=\"dragArea list-group\"\n        :list=\"list1\"\n        :clone=\"clone\"\n        :group=\"{ name: 'people', pull: pullFunction }\"\n        @start=\"start\"\n        item-key=\"id\"\n      >\n        <template #item=\"{ element }\">\n          <div class=\"list-group-item\">\n            {{ element.name }}\n          </div>\n        </template>\n      </draggable>\n    </div>\n\n    <div class=\"col-3\">\n      <h3>Draggable 2</h3>\n      <draggable\n        class=\"dragArea list-group\"\n        :list=\"list2\"\n        group=\"people\"\n        item-key=\"id\"\n      >\n        <template #item=\"{ element }\">\n          <div class=\"list-group-item\">\n            {{ element.name }}\n          </div>\n        </template>\n      </draggable>\n    </div>\n\n    <rawDisplayer class=\"col-3\" :value=\"list1\" title=\"List 1\" />\n\n    <rawDisplayer class=\"col-3\" :value=\"list2\" title=\"List 2\" />\n  </div>\n</template>\n\n<script>\nimport draggable from \"@/vuedraggable\";\nlet idGlobal = 8;\nexport default {\n  name: \"clone-on-control\",\n  display: \"Clone on Control\",\n  instruction: \"Press Ctrl to clone element from list 1\",\n  order: 4,\n  components: {\n    draggable\n  },\n  data() {\n    return {\n      list1: [\n        { name: \"Jesus\", id: 1 },\n        { name: \"Paul\", id: 2 },\n        { name: \"Peter\", id: 3 }\n      ],\n      list2: [\n        { name: \"Luc\", id: 5 },\n        { name: \"Thomas\", id: 6 },\n        { name: \"John\", id: 7 }\n      ],\n      controlOnStart: true\n    };\n  },\n  methods: {\n    clone({ name }) {\n      return { name, id: idGlobal++ };\n    },\n    pullFunction() {\n      return this.controlOnStart ? \"clone\" : true;\n    },\n    start({ originalEvent }) {\n      this.controlOnStart = originalEvent.ctrlKey;\n    }\n  }\n};\n</script>\n<style scoped></style>\n"],"sourceRoot":""}]}
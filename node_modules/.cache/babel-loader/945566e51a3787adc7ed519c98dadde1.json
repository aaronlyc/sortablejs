A,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,GAAA,IAAA,QAAA,CAAA,GAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAAmB,GAAlE,EAAsD,KAAtD,CACI,CAFN,CA6BI,EAzBJ,YAAA,CAsBM,KAtBN,EAAA,UAAA,EAsBM,CArBJ,YAAA,CAAqC,IAArC,EAAqC,IAArC,EAAI,eAAU,gBAAA,CAAG,QAAA,CAAA,YAAH,CAAd,EAA6B;AAAA;AAA7B,GAqBI,EAnBJ,YAAA,CAkBY,oBAlBZ,EAkBY;AAjBV,IAAA,GAAG,EAAC,IAiBM;AAhBT,IAAA,IAAI,EAAE,KAAA,CAAA,IAgBG;AAfV,IAAA,KAAK,EAAC,YAeI;AAdV,IAAA,MAAM,EAAC,SAcG;AAbV,gBAAS;AAaC,GAlBZ,E;AAOa,IAAA,IAAI,EAAA,OAAA,CACb;AAAA,UADiB,OACjB,QADiB,OACjB;AAAA,UAD0B,KAC1B,QAD0B,KAC1B;AAAA,aAD+B,CAC/B,YAAA,CAQK,IARL,EAAA,UAAA,EAQK,CAPH,UAOG,EALH,YAAA,CAA6C,MAA7C,EAAA,UAAA,EAA6C,gBAAA,CAAvB,OAAO,CAAC,IAAe,CAA7C,EAAkC;AAAA;AAAlC,OAKG,E,gBAHH,YAAA,CAAiE,OAAjE,EAAiE;AAA1D,QAAA,IAAI,EAAC,MAAqD;AAA9C,QAAA,KAAK,EAAC,cAAwC;;iBAAhB,OAAO,CAAC,IAAR,GAAY,M;;AAAI,OAAjE,E,IAAA,E;;AAAA,Q,uBAAA,C,iBAAiD,OAAO,CAAC,I,GAGtD,EADH,YAAA,CAA0D,GAA1D,EAA0D;AAAvD,QAAA,KAAK,EAAC,mBAAiD;AAA5B,QAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,iBAAE,QAAA,CAAA,QAAA,CAAS,KAAT,CAAF;AAAA;AAAuB,OAA1D,E,IAAA,E;;AAAA,Q,WAAA,CACG,CARL,CAD+B,CAC/B;AAAA,KADa,C;;;;GAPjB,E;;AAAA,I,QAAA,CAmBI,CAtBN,CAyBI,EADJ,YAAA,CAAyD,uBAAzD,EAAyD;AAA3C,IAAA,KAAK,EAAC,OAAqC;AAA5B,IAAA,KAAK,EAAE,KAAA,CAAA,IAAqB;AAAf,IAAA,KAAK,EAAC;AAAS,GAAzD,E,IAAA,E;;AAAA,I,SAAA,CACI,CA9BN,C","sourcesContent":["<template>\n  <div class=\"row\">\n    <div class=\"col-1\">\n      <button class=\"btn btn-secondary button\" @click=\"add\">Add</button>\n    </div>\n\n    <div class=\"col-7\">\n      <h3>Draggable {{ draggingInfo }}</h3>\n\n      <draggable\n        tag=\"ul\"\n        :list=\"list\"\n        class=\"list-group\"\n        handle=\".handle\"\n        item-key=\"name\"\n      >\n        <template #item=\"{ element, index }\">\n          <li class=\"list-group-item\">\n            <i class=\"fa fa-align-justify handle\"></i>\n\n            <span class=\"text\">{{ element.name }} </span>\n\n            <input type=\"text\" class=\"form-control\" v-model=\"element.text\" />\n\n            <i class=\"fa fa-times close\" @click=\"removeAt(index)\"></i>\n          </li>\n        </template>\n      </draggable>\n    </div>\n\n    <rawDisplayer class=\"col-3\" :value=\"list\" title=\"List\" />\n  </div>\n</template>\n\n<script>\nlet id = 3;\nimport draggable from \"@/vuedraggable\";\nexport default {\n  name: \"handle\",\n  display: \"Handle\",\n  instruction: \"Drag using the handle icon\",\n  order: 5,\n  components: {\n    draggable\n  },\n  data() {\n    return {\n      list: [\n        { name: \"John\", text: \"\", id: 0 },\n        { name: \"Joao\", text: \"\", id: 1 },\n        { name: \"Jean\", text: \"\", id: 2 }\n      ],\n      dragging: false\n    };\n  },\n  computed: {\n    draggingInfo() {\n      return this.dragging ? \"under drag\" : \"\";\n    }\n  },\n  methods: {\n    removeAt(idx) {\n      this.list.splice(idx, 1);\n    },\n    add: function() {\n      id++;\n      this.list.push({ name: \"Juan \" + id, id, text: \"\" });\n    }\n  }\n};\n</script>\n<style scoped>\n.button {\n  margin-top: 35px;\n}\n.handle {\n  float: left;\n  padding-top: 8px;\n  padding-bottom: 8px;\n}\n\n.close {\n  float: right;\n  padding-top: 8px;\n  padding-bottom: 8px;\n}\n\ninput {\n  display: inline-block;\n  width: 50%;\n}\n\n.text {\n  margin: 20px;\n}\n</style>\n"],"sourceRoot":""}]}
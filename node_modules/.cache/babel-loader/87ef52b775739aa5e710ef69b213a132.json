E,QAAA,CAAA,IAAA,IAAA,QAAA,CAAA,IAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAEtC,GAFT,EAAuD,qBAAvD,CAGI,CAJN,CAkCI,EA5BJ,YAAA,CAyBM,KAzBN,EAAA,UAAA,EAyBM,CAxBJ,UAwBI,EAvBJ,YAAA,CAsBY,oBAtBZ,EAAA,WAAA,CAsBY;AArBV,IAAA,KAAK,EAAC,YAqBI;AApBV,gBAAS,OAoBC;AAnBV,IAAA,GAAG,EAAC,kBAmBM;AAlBT,sBAAgB;AAAA,MAAA,GAAA,EAAA,IAAA;AAAA,MAAA,IAAA,EAAA,WAAA;AAAA,MAAA,IAAA,EAAA;AAAA,KAkBP;gBAjBD,KAAA,CAAA,IAiBC;;aAjBD,KAAA,CAAA,IAAA,GAAI,M;;AAiBH,GAtBZ,EAMU,QAAA,CAAA,WANV,EAMqB;AAClB,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,IAAA,CAAA,UAAA,GAAU,IAAZ;AAAA,KAAA,CADa;AAElB,IAAA,KAAG,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,IAAA,CAAA,UAAA,GAAU,KAAZ;AAAA,KAAA;AAFe,GANrB,CAAA,E;AAUa,IAAA,IAAI,EAAA,QAAA,CACb;AAAA,UADiB,OACjB,QADiB,OACjB;AAAA,aADwB,CACxB,YAAA,CASK,IATL,EAAA,UAAA,EASK,CARH,YAAA,CAMK,GANL,EAMK;AALF,QAAA,KAAK,EAAmB,OAAO,CAAC,KAAR,GAAa,cAAb,GAAa,6BAKnC;AAFF,QAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,iBAAE,OAAO,CAAC,KAAR,GAAa,CAAI,OAAO,CAAC,KAA3B;AAAA,SAEH;AADH,uBAAY;AACT,OANL,E,IAAA,E;;AAAA,Q,WAAA,CAQG,E,iBAFE,MACL,gBAAA,CAAG,OAAO,CAAC,IAAX,C,EAAe;AAAA;OACZ,CATL,CADwB,CACxB;AAAA,KADa,C;;;;GAVjB,E;;AAAA,I,cAAA,CAuBI,CAzBN,CA4BI,EADJ,YAAA,CAAyD,uBAAzD,EAAyD;AAA3C,IAAA,KAAK,EAAC,OAAqC;AAA5B,IAAA,KAAK,EAAE,KAAA,CAAA,IAAqB;AAAf,IAAA,KAAK,EAAC;AAAS,GAAzD,E,IAAA,E;;AAAA,I,SAAA,CACI,CAnCN,C","sourcesContent":["<template>\n  <div class=\"row\">\n    <div class=\"col-2\">\n      <button class=\"btn btn-secondary button\" @click=\"sort\">\n        To original order\n      </button>\n    </div>\n\n    <div class=\"col-6\">\n      <h3>Transition</h3>\n      <draggable\n        class=\"list-group\"\n        item-key=\"order\"\n        tag=\"transition-group\"\n        :component-data=\"{ tag: 'ul', name: 'flip-list', type: 'transition' }\"\n        v-model=\"list\"\n        v-bind=\"dragOptions\"\n        @start=\"isDragging = true\"\n        @end=\"isDragging = false\"\n      >\n        <template #item=\"{ element }\">\n          <li class=\"list-group-item\">\n            <i\n              :class=\"\n                element.fixed ? 'fa fa-anchor' : 'glyphicon glyphicon-pushpin'\n              \"\n              @click=\"element.fixed = !element.fixed\"\n              aria-hidden=\"true\"\n            ></i>\n            {{ element.name }}\n          </li>\n        </template>\n      </draggable>\n    </div>\n\n    <rawDisplayer class=\"col-3\" :value=\"list\" title=\"List\" />\n  </div>\n</template>\n\n<script>\nimport draggable from \"@/vuedraggable\";\nconst message = [\n  \"vue.draggable\",\n  \"draggable\",\n  \"component\",\n  \"for\",\n  \"vue.js 2.0\",\n  \"based\",\n  \"on\",\n  \"Sortablejs\"\n];\nlet order = message.length;\n\nexport default {\n  name: \"transition-example\",\n  display: \"Transition\",\n  order: 6,\n  components: {\n    draggable\n  },\n  data() {\n    return {\n      list: message.map((name, index) => {\n        return { name, order: index + 1 };\n      })\n    };\n  },\n  methods: {\n    sort() {\n      this.list = this.list.sort((a, b) => a.order - b.order);\n    }\n  },\n  computed: {\n    dragOptions() {\n      return {\n        animation: 0,\n        group: \"description\",\n        disabled: false,\n        ghostClass: \"ghost\"\n      };\n    }\n  }\n};\n</script>\n\n<style>\n.button {\n  margin-top: 35px;\n}\n\n.flip-list-move {\n  transition: transform 0.5s;\n}\n\n.no-move {\n  transition: transform 0s;\n}\n\n.ghost {\n  opacity: 0.5;\n  background: #c8ebfb;\n}\n\n.list-group {\n  min-height: 20px;\n}\n\n.list-group-item {\n  cursor: move;\n}\n\n.list-group-item i {\n  cursor: pointer;\n}\n</style>\n"],"sourceRoot":""}]}